{
  "extends": [
    "@beyondessential/eslint-config-beyondessential"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  overrides: [
    {
      files: [
        "**/*.test.js"
      ],
      env: {
        jest: true // now **/*.test.js files' env has both es6 *and* jest
      },
      plugins: ["jest"],
    }
  ],
}

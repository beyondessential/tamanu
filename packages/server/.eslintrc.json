{
    "extends": "airbnb-base",
    "env": {
        "jest": true
    },
    "rules": {
        "arrow-body-style": "off",
        "function-paren-newline": "off",
        "global-require": "off",
        "indent": "off",
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-multi-spaces": "off",
        "no-param-reassign": "off",
        "object-curly-newline": "off",
        "comma-dangle": "off",
        "class-methods-use-this": "off"
    }
}

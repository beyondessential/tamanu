    "config": "^3.3.9",
    "google-spreadsheet": "^2.0.7",
    "inflection": "^1.13.4",
    "lodash": "^4.17.5",
    "pg": "^8.5.1",
    "pgsql-ast-parser": "^11.1.0",
    "rimraf": "^6.0.1",
    "supertest": "^4.0.2",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@types/supertest": "^6.0.3",

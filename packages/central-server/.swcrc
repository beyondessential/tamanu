{
  "$schema": "https://json.schemastore.org/swcrc",
  "exclude": ["node_modules/"],
  "module": {
    "type": "commonjs",
    "aliases": {
      "promise": {
        "global": "Promise"
      }
    }
  },
  "jsc": {
    "parser": {
      "syntax": "ecmascript",
      "jsx": true
    },
    "target": "es2020",
    "keepClassNames": true
  }
}

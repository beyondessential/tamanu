{
  "extends": "../../common.tsconfig.json",
  "include": ["src/**/*.ts"],
  "compilerOptions": {
    "baseUrl": "./src",
    "strict": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "isolatedModules": false,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "target": "es2020",
    "moduleResolution": "bundler",
    "module": "ESNext"
  }
}

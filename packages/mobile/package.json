{
  "name": "tamanuapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint .",
    "storybook": "start-storybook -p 7007",
    "prestorybook": "rnstl"
  },
  "dependencies": {
    "date-fns": "^2.7.0",
    "react": "16.9.0",
    "react-native": "0.61.4",
    "react-native-collapsible": "^1.5.1",
    "react-native-dotenv": "^0.2.0",
    "react-native-fast-image": "^7.0.2",
    "react-native-gesture-handler": "^1.5.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-masked-text": "^1.13.0",
    "react-native-pose": "^0.9.1",
    "react-native-reanimated": "^1.4.0",
    "react-native-svg": "^9.13.3",
    "react-navigation": "^4.0.10",
    "react-navigation-stack": "^1.10.3",
    "react-navigation-tabs": "^2.5.6",
    "styled-components": "^4.4.1",
    "styled-system": "^5.1.2",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-transform-modules-commonjs": "^7.7.0",
    "@babel/runtime": "^7.7.2",
    "@beyondessential/eslint-config-beyondessential": "^2.3.0",
    "@react-native-community/eslint-config": "^0.0.5",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/addons": "^5.2.6",
    "@storybook/react-native": "^5.2.6",
    "@storybook/react-native-server": "^5.2.6",
    "@testing-library/react-hooks": "^3.2.1",
    "@testing-library/react-native": "^5.0.1",
    "@types/jest": "^24.0.23",
    "@types/react": "^16.9.11",
    "@types/react-native": "^0.60.22",
    "@types/react-test-renderer": "^16.9.1",
    "@types/styled-components": "^4.4.0",
    "@types/styled-system": "^5.1.4",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.6.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.57.0",
    "react-dom": "16.9.0",
    "react-native-storybook-loader": "^1.8.1",
    "react-native-svg-mock": "^2.0.0",
    "react-native-svg-transformer": "^0.13.0",
    "react-test-renderer": "16.9.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.7.2"
  },
  "jest": {
    "preset": "@testing-library/react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|react-pose-core|react-native-reanimated|react-native-pose|animated-pose|react-native-fast-image|react-native-collapsible|)/)"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "(/App/.*\\.test)\\.(ts|tsx|js)$",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@?react-navigation|react-pose-core|animated-pose)"
    ],
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./App"
      ],
      "pattern": "**/*.stories.tsx",
      "outputFile": "./storybook/storyLoader.js"
    }
  }
}

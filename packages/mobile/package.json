{
  "name": "@tamanu/mobile",
  "version": "2.37.0",
  "private": true,
  "scripts": {
    "android": "npx react-native start --reset-cache",
    "build:android": "cd android && ./gradlew clean && ./gradlew app:assembleRelease && cd ..",
    "clean:deps": "rimraf node_modules",
    "start": "rnstl --silent && react-native start",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "test:unit": "npm run test -- --watch -c jest-unit-config.js",
    "test:integration": "npm run test -- --watch -c jest-integration-config.js",
    "test:commit": "npm run test -- --findRelatedTests",
    "test:watch": "npm run test -- --watchAll",
    "e2e:build": "detox build",
    "test:e2e": "detox test",
    "typecheck": "tsc --project tsconfig.json",
    "storybook": "cat storybook/script-usage-instructions.md && (adb reverse tcp:7007 tcp:7007 || true) && start-storybook",
    "prestorybook": "rnstl",
    "postinstall": "patch-package",
    "migrate-create": "../../scripts/create_mobile_migration.sh"
  },
  "postinstall": "cd ../.. && npm run prepare",
